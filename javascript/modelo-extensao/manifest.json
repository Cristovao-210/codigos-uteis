{
  "name": "Título da extensão",
  "description": "Resumo do que a extensão faz",
  "version": "1.0",
  "manifest_version": 3,
  "author": "Nome do autor",
  "content_scripts": [
    {
      "matches": ["https://caminho.pagina.br/*"],
      "js": ["scripts/content.js"]
    }
  ],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {  
      "128": "images/icon128.png"
    }
  }
}


